<svg width="48" height="48" viewBox="0 0 48 48" xmlns="http://www.w3.org/2000/svg">
  <!-- White background -->
  <rect width="48" height="48" fill="#FFFFFF"/>
  
  <!-- Building with 3D perspective - scaled for favicon -->
  <g transform="translate(24, 24)">
    <!-- Main building face -->
    <g transform="scale(0.35)">
      <!-- Red stripe 1 -->
      <path d="M -20 -25 L 20 -30 L 20 -15 L -20 -10 Z" fill="#CC0000"/>
      <!-- White stripe 1 -->
      <path d="M -20 -10 L 20 -15 L 20 0 L -20 5 Z" fill="#FFFFFF" stroke="#CC0000" stroke-width="0.5"/>
      <!-- Red stripe 2 -->
      <path d="M -20 5 L 20 0 L 20 15 L -20 20 Z" fill="#CC0000"/>
      <!-- White stripe 2 -->
      <path d="M -20 20 L 20 15 L 20 30 L -20 35 Z" fill="#FFFFFF" stroke="#CC0000" stroke-width="0.5"/>
      
      <!-- Building outline -->
      <path d="M -20 -25 L 20 -30 L 20 30 L -20 35 Z" fill="none" stroke="#CC0000" stroke-width="1"/>
      
      <!-- 3D side face -->
      <path d="M 20 -30 L 25 -25 L 25 35 L 20 30 Z" fill="#AA0000" stroke="#CC0000" stroke-width="0.5"/>
      
      <!-- 3D top face -->
      <path d="M -20 -25 L -15 -30 L 25 -25 L 20 -30 Z" fill="#DD0000" stroke="#CC0000" stroke-width="0.5"/>
    </g>
  </g>
  
  <!-- OWN PARKS text - smaller for favicon -->
  <text x="24" y="42" font-family="Arial, sans-serif" font-size="4" font-weight="bold" text-anchor="middle" fill="#1a237e" letter-spacing="0.5px">
    OWN PARKS
  </text>
</svg>