<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- White background -->
  <rect width="512" height="512" fill="#FFFFFF"/>
  
  <!-- Building with 3D perspective - centered -->
  <g transform="translate(256, 256)">
    <!-- Main building face -->
    <g transform="scale(1.8)">
      <!-- Red stripe 1 -->
      <path d="M -50 -40 L 50 -50 L 50 -25 L -50 -15 Z" fill="#CC0000"/>
      <!-- White stripe 1 -->
      <path d="M -50 -15 L 50 -25 L 50 0 L -50 10 Z" fill="#FFFFFF" stroke="#CC0000" stroke-width="0.5"/>
      <!-- Red stripe 2 -->
      <path d="M -50 10 L 50 0 L 50 25 L -50 35 Z" fill="#CC0000"/>
      <!-- White stripe 2 -->
      <path d="M -50 35 L 50 25 L 50 50 L -50 60 Z" fill="#FFFFFF" stroke="#CC0000" stroke-width="0.5"/>
      <!-- Red stripe 3 -->
      <path d="M -50 60 L 50 50 L 50 75 L -50 85 Z" fill="#CC0000"/>
      
      <!-- Building outline -->
      <path d="M -50 -40 L 50 -50 L 50 75 L -50 85 Z" fill="none" stroke="#CC0000" stroke-width="2"/>
      
      <!-- 3D side face -->
      <path d="M 50 -50 L 65 -40 L 65 85 L 50 75 Z" fill="#AA0000" stroke="#CC0000" stroke-width="1"/>
      
      <!-- 3D top face -->
      <path d="M -50 -40 L -35 -50 L 65 -40 L 50 -50 Z" fill="#DD0000" stroke="#CC0000" stroke-width="1"/>
    </g>
  </g>
  
  <!-- OWN PARKS text -->
  <text x="256" y="420" font-family="Arial, sans-serif" font-size="36" font-weight="bold" text-anchor="middle" fill="#1a237e" letter-spacing="4px">
    OWN PARKS
  </text>
</svg>